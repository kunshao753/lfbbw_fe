<scroll-view class="scroll-v list" enableBackToTop="true" scroll-y="{{true}}" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{cellData.data}}" wx:for-item="newsitem" wx:for-index="index2" wx:key="id"><view class="school-cell"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['cellData.data','id',newsitem.id]]]]]]]}}" class="media-item uni-flex" bindtap="__e"><image class="image-list" src="{{newsitem.image_url}}"></image><view class="school-r uni-flex-item"><view class="uni-flex uni-space-between title-box"><text class="uni-title">{{newsitem.title}}</text><text class="uni-title">{{newsitem.score+"分"}}</text></view><view class="uni-flex uni-space-between"><view class="uni-flex uni-row"><block wx:for="{{newsitem.label}}" wx:for-item="tag" wx:for-index="tindex"><text class="school-tag">{{tag}}</text></block></view><text>{{newsitem.percent}}</text></view><view class="uni-flex uni-acenter mt10"><image class="sd-addressicon" src="../../../static/location.png"></image><text>{{newsitem.location}}</text></view></view></view><view data-event-opts="{{[['tap',[['goMajor',['$0'],[[['cellData.data','id',newsitem.id]]]]]]]}}" class="uni-inline-item uni-jcenter matching" bindtap="__e">匹配专业<text>{{newsitem.matchingMajor}}</text><uni-icons vue-id="{{'4fc6ef52-1-'+index2}}" type="arrowright" size="10" color="#828282" bind:__l="__l"></uni-icons></view></view></block><block wx:if="{{cellData.isLoading||cellData.data.length>4}}"><view class="loading-more"><text class="loading-more-text">{{cellData.loadingText}}</text></view></block></scroll-view>